{
  "name": "fit-website-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://0Goldens0.github.io/React-FIT",
  "scripts": {
    "dev": "next dev -p 3000",
    "server": "node server/index.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run server\"",
    "build": "next build",
    "start": "next start -p 3000",
    "cms:up": "docker compose -f docker-compose.cms.yml up -d",
    "cms:down": "docker compose -f docker-compose.cms.yml down",
    "cms:logs": "docker compose -f docker-compose.cms.yml logs -f cms",
    "cms:build": "npm --prefix cms run build",
    "cms:start": "npm --prefix cms run start",
    "convert-images": "node scripts/convert-to-webp.js",
    "cleanup-images": "node scripts/cleanup-originals.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "framer-motion": "^11.15.0",
    "lucide-react": "^0.469.0",
    "next": "^15.5.3",
    "nodemailer": "^6.9.9",
    "pdfjs-dist": "^4.10.38",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "autoprefixer": "^10.4.22",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-config-next": "^15.5.9",
    "eslint-plugin-react-hooks": "^5.1.0",
    "postcss": "^8.5.6",
    "sharp": "^0.33.5",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.2.2"
  }
}
