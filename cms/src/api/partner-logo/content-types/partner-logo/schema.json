{
  "kind": "collectionType",
  "collectionName": "partner_logos",
  "info": {
    "singularName": "partner-logo",
    "pluralName": "partner-logos",
    "displayName": "Partner Logo",
    "description": "Logos for the partners marquee"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "url": {
      "type": "string"
    },
    "order": {
      "type": "integer",
      "default": 0
    }
  }
}

